{
  "rewrites": [
    {
      "source": "/places/:path*",
      "destination": "/places/:path*"
    },
    {
      "source": "/_next/static/:path*",
      "destination": "/_next/static/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps"
}
