{
  "rewrites": [
    {
      "source": "/places/:path*",
      "has": [
        {
          "type": "header",
          "key": "x-no-rewrite"
        }
      ],
      "destination": "/places/:path*"
    },
    {
      "source": "/places/:path*",
      "destination": "https://muft-ssr.vercel.app/places/:path*",
      "headers": {
        "x-no-rewrite": "true"
      }
    }
  ]
}
