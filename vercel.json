{
  "rewrites": [
    {
      "source": "/places/:path*",
      "destination": "https://muft-theta.vercel.app/places/:path*"
    },
    {
      "source": "/_next/:path*",
      "destination": "https://muft-theta.vercel.app/_next/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://muffun-test.vercel.app/api/:path*"
    }
  ]
}
